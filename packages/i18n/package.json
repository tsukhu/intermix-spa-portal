{
  "scripts": {
    "start": "webpack-dev-server",
    "clean": "rimraf dist && rimraf ../../dist/i18n",
    "copy-for-deploy": "copyfiles -a -f ./dist/* ../../dist/i18n",
    "build": "npm run clean && webpack --mode=production && npm run copy-for-deploy",
    "analyze": "webpack --mode=production --env.analyze=true",
    "lint": "eslint src",
    "prettier": "prettier --write './**'",
    "test": "jest --passWithNoTests",
    "watch-tests": "jest --watch",
    "coverage": "jest --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && concurrently yarn:test yarn:lint"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.10.1",
    "@babel/runtime": "^7.8.7",
    "@types/jest": "^24.0.23",
    "angular-i18next": "^9.0.1",
    "babel-eslint": "^11.0.0-beta.2",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "concurrently": "^5.0.1",
    "css-loader": "^3.3.2",
    "eslint": "^6.7.2",
    "eslint-config-important-stuff": "^1.1.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-ts-react-important-stuff": "^2.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "style-loader": "^1.0.1",
    "systemjs-webpack-interop": "^1.1.2",
    "typescript": "^3.9.5",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-config-single-spa": "^1.17.3",
    "webpack-config-single-spa-ts": "^1.17.3",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "i18next": "^19.5.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-i18next": "^11.7.0"
  },
  "name": "i18n",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Tarun Kumar Sukhu",
  "license": "MIT"
}
