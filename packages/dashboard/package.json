{
  "scripts": {
    "start": "webpack-dev-server",
    "clean": "rimraf dist && rimraf ../../dist/dashboard",
    "copy-for-deploy": "copyfiles -a -f ./dist/* ../../dist/dashboard && copyfiles -a -f ./dist/fonts/* ../../dist/dashboard/fonts",
    "build": "npm run clean && webpack --mode=production && npm run copy-for-deploy",
    "analyze": "webpack --mode=production --env.analyze=true",
    "lint": "eslint src",
    "prettier": "prettier --write './**'",
    "test": "jest --passWithNoTests",
    "watch-tests": "jest --watch",
    "coverage": "jest --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && concurrently yarn:test yarn:lint"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.10.1",
    "@babel/runtime": "^7.8.7",
    "@testing-library/react": "^9.4.0",
    "@types/date-fns": "^2.6.0",
    "@types/jest": "^24.0.23",
    "@types/react": "^16.9.49",
    "@typescript-eslint/parser": "^3.4.0",
    "babel-eslint": "^11.0.0-beta.2",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "concurrently": "^5.0.1",
    "css-loader": "^4.3.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-react-important-stuff": "^2.0.0",
    "eslint-config-ts-react-important-stuff": "^2.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "node-sass": "^4.14.1",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "sass-loader": "^8.0.2",
    "single-spa-react": "^3.1.0",
    "style-loader": "^1.2.1",
    "systemjs-webpack-interop": "^1.1.2",
    "typescript": "^3.9.5",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-config-single-spa-react": "^1.17.0",
    "webpack-config-single-spa-react-ts": "^1.17.0",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2",
    "xml-loader": "^1.2.1"
  },
  "dependencies": {
    "bpmn-js": "^7.3.0",
    "date-fns": "^2.16.1",
    "react": "^16.12.0",
    "react-bpmn": "^0.2.0",
    "react-hook-form": "^6.8.3",
    "react-i18next": "^11.7.0"
  },
  "name": "dashboard",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Tarun Kumar Sukhu <tksukhu@gmail.com>",
  "license": "MIT"
}